   "source": [
    "corpus_m = list()\n",
    "for txt_file in txt_file_list:\n",
    "    corpus = []\n",
    "    with open(txt_file, 'r', encoding='utf-8') as f:\n",
    "        #print(f.read())\n",
    "        for line in f:\n",
    "            corpus.append(\" \".join(jieba.cut(line.split(',')[0], cut_all=False)))\n",
    "            \n",
    "    corpus_m.append(corpus)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": false
   },
