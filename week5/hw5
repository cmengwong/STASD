    "collapsed": false
   },
   "outputs": [],
   "source": [
    "dic_vector_s = list()\n",
    "dic_mat = pd.DataFrame\n",
    "for corpus in corpus_m:\n",
    "    dic_vector = {}\n",
    "    for c in corpus:\n",
    "        seg_list = jieba.cut(c) \n",
    "        string_list = \"|\".join(seg_list).split('|')\n",
    "        for s in string_list:\n",
    "            if not (s in dic_vector.keys()):\n",
    "                dic_vector[s] = 1\n",
    "            else:\n",
    "                dic_vector[s] += 1\n",
    "    dic_vector_s.append(dic_vector)\n",
    "                "
   ]
  },
